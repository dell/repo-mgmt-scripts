;
; This is a yum repository configuration file.
;
; Drop this file into /etc/yum/yum.repos.d/ to enable this repository
;
[dell-hardware-PENAME]
name=Dell unofficial hardware repository
mirrorlist=http://linux.dell.com/repo/hardware/mirrors.pl?dellname=PENAME&osname=OSNAME.$basearch&dellsysidpluginver=$dellsysidpluginver
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-dell
       file:///etc/pki/rpm-gpg/RPM-GPG-KEY-libsmbios

